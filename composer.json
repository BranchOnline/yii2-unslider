{
  "name": "branchonline/yii2-unslider",
  "description": "Unslider widget for Yii2",
  "keywords": [
    "yii",
    "yii2",
    "widget",
    "unslider"
  ],
  "homepage": "https://github.com/BranchOnline/yii2-unslider",
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Jap Mul",
      "email": "jap@branchonline.nl"
    }
  ],
  "require": {
    "yiisoft/yii2": "*",
    "bower-asset/jquery.unslider": "dev-master#3cf59826f309fc4815409d47cc53b803cc44dfc2",
    "bower-asset/jquery.event.swipe": "0.5.4"
  },
  "config": {
    "fxp-asset": {
      "repositories": {
        "bower-asset/jquery.unslider": {
          "type": "bower-vcs",
          "url": "git@github.com:/idiot/unslider.git"
        }
      }
    }
  },
  "autoload": {
    "psr-4": {
      "branchonline\\unslider\\": "src/"
    }
  }
}
